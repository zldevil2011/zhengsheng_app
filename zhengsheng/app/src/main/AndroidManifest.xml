<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.newly_dawn.app.zhengsheng">

    <uses-sdk tools:overrideLibrary="com.yalantis.pulltorefresh.library" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".MainActivity" />
        <activity android:name=".welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".user.mine" />
        <activity android:name=".index" />
        <activity android:name=".data.data" />
        <activity
            android:name=".user.Register"
            android:label="@string/title_activity_register"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".user.login"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".user.WorkOrder"
            android:label="@string/title_activity_work_order"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".user.Alarm"
            android:label="@string/title_activity_alarm"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".user.Contactus"
            android:label="@string/title_activity_contactus"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".user.personalInfo"
            android:label="@string/title_activity_personal_info"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".user.feedback"
            android:label="@string/title_activity_feedback"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".data.data_list"
            android:label="@string/title_activity_data_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".tools.Browser"
            android:label="@string/title_activity_browser"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>